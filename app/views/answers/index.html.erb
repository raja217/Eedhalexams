<div id="content-header">
  <%= image_tag("/images/examination/show_exam.png") %>
  <h1><%= t('online_exam') %></h1>
  <h3><%= t('home') %></h3>
  <div id="app-back-button">
    <%= link_to_function image_tag("/images/buttons/back.png",:border => 0), "history.back()" %>
  </div>
</div>
<div id="page-yield">
  <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>
  <div class="box">
       <div class="link-box">
      <div class="link-heading">
        <div class="label-container"><%= t('name') %> : <%= @exams.name %></div><br> <div class="label-container"><%= "Exam Date" %> : <%= @exams.exam_date %></div> <br> <div class="label-container"><%= "Total Time" %> : <%= @exams.time %> </div><br>
      </div>
    </div>
    <div id="skip">
       <%= link_to "â–º Start Exam", exam_exam_group_answers_path(@exams), :id => @exams.id  %>
     </div>


    </div>
    <div id="update_exam"></div>
    <div class="extender"></div>
  </div>
</div>
